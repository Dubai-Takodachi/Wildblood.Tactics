{
  "profiles": {
    "Docker Compose": {
      "commandName": "DockerCompose",
      "commandVersion": "1.0",
      "composeLaunchAction": "LaunchBrowser",
      "composeLaunchServiceName": "wildblood.tactics",
      "composeLaunchUrl": "{Scheme}://localhost:{ServicePort}",
      "inspectUri": "{Scheme}://localhost:{ServicePort}/_framework/debug/ws-proxy?browser={browserInspectUri}",
      "serviceActions": {
        "wildblood.tactics": "StartDebugging",
        "mongodb": "StartWithoutDebugging",
        "mssql": "StartWithoutDebugging"
      }
    }
  }
}