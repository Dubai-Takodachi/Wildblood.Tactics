# Wildblood.Tactics Deployment Environment Configuration
# Copy this file to .env and fill in your production values
# 
# IMPORTANT: Never commit the actual .env file to git!

# Domain Configuration
DOMAIN=Wildblood-Tactics.de

# MongoDB Configuration
MONGO_USERNAME=wildblood
MONGO_PASSWORD=your_secure_mongo_password_here

# SQL Server Configuration  
SQL_PASSWORD=your_secure_sql_password_here

# Email (SMTP) Configuration
EMAIL_SMTP_SERVER=mxe9ad.netcup.net
EMAIL_SMTP_PORT=465
EMAIL_SMTP_USER=no-reply@wildblood-tactics.de
EMAIL_SMTP_PASSWORD=your_secure_email_password_here
EMAIL_FROM_NAME=Wildblood Tactics

# Example values:
# DOMAIN=my-domain.com
# MONGO_USERNAME=myuser
# MONGO_PASSWORD=SecureMongoPass123!
# SQL_PASSWORD=SecureSqlPass456!
# EMAIL_SMTP_SERVER=smtp.example.com
# EMAIL_SMTP_PORT=587
# EMAIL_SMTP_USER=noreply@example.com
# EMAIL_SMTP_PASSWORD=SecureEmailPass789!
# EMAIL_FROM_NAME=My Application